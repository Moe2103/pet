<div class="signers">
    <h1>Signers ({{Signers}}):</h1>
    <ul">
        {{#each allData}}
            <li>{{#if url}}<a href="{{url}}" target="_blank">{{first}} {{last}}</a>{{else}}{{first}} {{last}}{{/if}}
            {{#if age}}age: {{age}}{{/if}}
            {{#if city}}<a href="/signers/{{city}}" target="_blank">{{city}}</a>{{/if}}
            </li>
        {{/each}}
    </ul>
</div>